<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>kanCollections</servlet-name>
  <servlet-class> org.springframework.web.servlet.DispatcherServlet </servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetPicture</servlet-name>
  	<display-name>GetPicture</display-name>
  	<description></description>
  	<servlet-class>com.kanCollections.kans.dao.GetPicture</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetSearchPic</servlet-name>
  	<display-name>GetSearchPic</display-name>
  	<description></description>
  	<servlet-class>com.kanCollections.kans.dao.GetSearchPic</servlet-class>
  </servlet>
 
  <servlet-mapping><servlet-name>kanCollections</servlet-name>
  <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetPicture</servlet-name>
  	<url-pattern>/GetPicture</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetSearchPic</servlet-name>
  	<url-pattern>/GetSearchPic</url-pattern>
  </servlet-mapping>
  
</web-app>
